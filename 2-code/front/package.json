{
  "name": "maj-angular",
  "version": "0.0.0",
  "author": {
    "name": "Guillaume TALBOT",
    "email": "talbotgui@gmail.com",
    "url": "https://guillaumetalbot.com"
  },
  "scripts": {
    "//install-* : commandes utiles pour faire démarrer les tests avec Cypress": "",
    "install-1-cypress": "cypress cache clear",
    "install-2-cypress": "cypress install",
    "//cypress : pour démarrer Cypress": "",
    "cypress": "cypress open",
    "//*-start : liste des scripts de démarrage de démarche": "",
    "adminpsl-start": "                    ng serve --host=admin.dev-psl.guillaumetalbot.com --port=443 --ssl=true --ssl-cert=../.certificatsLocaux/tls/dev-certificatDeDeveloppementSigne.crt --ssl-key=../.certificatsLocaux/tls/dev-clefDeDeveloppement.key --serve-path=mademarche/adminpsl/                     --project=adminpsl",
    "edition-start": "                     ng serve --host=admin.dev-psl.guillaumetalbot.com --port=443 --ssl=true --ssl-cert=../.certificatsLocaux/tls/dev-certificatDeDeveloppementSigne.crt --ssl-key=../.certificatsLocaux/tls/dev-clefDeDeveloppement.key --serve-path=mademarche/edition/                      --project=edition",
    "generique-start": "                   ng serve --host=dev-psl.guillaumetalbot.com       --port=443 --ssl=true --ssl-cert=../.certificatsLocaux/tls/dev-certificatDeDeveloppementSigne.crt --ssl-key=../.certificatsLocaux/tls/dev-clefDeDeveloppement.key --serve-path=mademarche/generique/                    --project=generique",
    "specifique-start": "       ng serve --host=dev-psl.guillaumetalbot.com       --port=443 --ssl=true --ssl-cert=../.certificatsLocaux/tls/dev-certificatDeDeveloppementSigne.crt --ssl-key=../.certificatsLocaux/tls/dev-clefDeDeveloppement.key --serve-path=mademarche/specifique/        --project=specifique",
    "//*-build-prod : pour builder la version PROD de la démarche": "",
    "all-build-prod": "npm run adminpsl-build-prod && npm run edition-build-prod && npm run generique-build-prod && npm run specifique-build-prod",
    "adminpsl-build-prod": "                    ng build --configuration production --aot --subresource-integrity --project=adminpsl",
    "edition-build-prod": "                     ng build --configuration production --aot --subresource-integrity --project=edition",
    "generique-build-prod": "                   ng build --configuration production --aot --subresource-integrity --project=generique",
    "specifique-build-prod": "       ng build --configuration production --aot --subresource-integrity --project=specifique",
    "//http-start": "Démarrer un serveur exposant les applications buildées",
    "http-start": "http-server ./dist/ -a dev-psl.guillaumetalbot.com -d false -p 443 -S -C ../.certificatsLocaux/tls/dev-certificatDeDeveloppementSigne.crt -K ../.certificatsLocaux/tls/dev-clefDeDeveloppement.key",
    "//*-compodoc : génération de la documentation du projet": "",
    "all-compodoc": "npm run framework-compodoc && npm run frameworktest-compodoc && npm run adminpsl-compodoc && npm run edition-compodoc && npm run generique-compodoc && npm run specifique-compodoc",
    "framework-compodoc": "            npx compodoc --language fr-FR -p ./projects/framework/tsconfig.doc.json             -d ../../1-conception/static/documentationgeneree/framework/compodoc",
    "frameworktest-compodoc": "        npx compodoc --language fr-FR -p ./projects/framework/tsconfig.doctest.json         -d ../../1-conception/static/documentationgeneree/frameworktest/compodoc",
    "adminpsl-compodoc": "             npx compodoc --language fr-FR -p ./projects/adminpsl/tsconfig.doc.json              -d ../../1-conception/static/documentationgeneree/adminpsl/compodoc",
    "edition-compodoc": "              npx compodoc --language fr-FR -p ./projects/edition/tsconfig.doc.json               -d ../../1-conception/static/documentationgeneree/edition/compodoc",
    "generique-compodoc": "            npx compodoc --language fr-FR -p ./projects/generique/tsconfig.doc.json             -d ../../1-conception/static/documentationgeneree/generique/compodoc",
    "specifique-compodoc": "npx compodoc --language fr-FR -p ./projects/specifique/tsconfig.doc.json -d ../../1-conception/static/documentationgeneree/specifique/compodoc",
    "//*-analyseBundle : génération d'un json détaillant le bundle et analyse du json (uniquement pour les applications et pas les librairies) (fichier non embarqué dans l'applicatif)": "",
    "all-analyseBundle": "npm run adminpsl-analyseBundle && npm run edition-analyseBundle && npm run generique-analyseBundle && npm run specifique-analyseBundle",
    "adminpsl-analyseBundle": "                    ng build --configuration=analyze-bundle --project=adminpsl --output-path=dist/analyseBundle/adminpsl/                                         && source-map-explorer --no-border-checks --html ../../1-conception/static/documentationgeneree/adminpsl/webpack/report.html dist/analyseBundle/adminpsl/**/*.js",
    "edition-analyseBundle": "                     ng build --configuration=analyze-bundle --project=edition --output-path=dist/analyseBundle/edition/                                           && source-map-explorer --no-border-checks --html ../../1-conception/static/documentationgeneree/edition/webpack/report.html dist/analyseBundle/edition/**/*.js",
    "generique-analyseBundle": "                   ng build --configuration=analyze-bundle --project=generique --output-path=dist/analyseBundle/generique/                                       && source-map-explorer --no-border-checks --html ../../1-conception/static/documentationgeneree/generique/webpack/report.html dist/analyseBundle/generique/**/*.js",
    "specifique-analyseBundle": "       ng build --configuration=analyze-bundle --project=specifique --output-path=dist/analyseBundle/specifique/               && source-map-explorer --no-border-checks --html ../../1-conception/static/documentationgeneree/specifique/webpack/report.html dist/analyseBundle/specifique/**/*.js",
    "//cloc : Comptage du nombre de lignes de code (pour debug, ajouter --found=found.txt --ignored=ignored.txt)": "",
    "//cloc : A synchroniser avec le .gitignore": "",
    "cloc": "                             npx cloc ../..           --exclude-dir=bouchonapi,db,target,.elastic,.analyse,.cache,.embedmongo,.log,.settings,.tomcatembarque,.project,.settings,dist,node_modules,node,.angular,.nyc_output,.vscode,_gen,tmp,documentationgeneree,installationdeposte,.certificatsLocaux,coverage,themes,package-lock.json,dependencyTrack --exclude-ext=project,classpath,lock,rdb,nyc_output,lnk,zip,exe,cmd,cjs,mjs | sed -n '/github.*/,//p' >../../1-conception/static/documentationgeneree/countLinesOfCode.txt",
    "clocManuelPourAnalyseDeLaCommande": "npx cloc ../.. --by-file --exclude-dir=bouchonapi,db,target,.elastic,.analyse,.cache,.embedmongo,.log,.settings,.tomcatembarque,.project,.settings,dist,node_modules,node,.angular,.nyc_output,.vscode,_gen,tmp,documentationgeneree,installationdeposte,.certificatsLocaux,coverage,themes,package-lock.json,dependencyTrack --exclude-ext=project,classpath,lock,rdb,nyc_output,lnk,zip,exe,cmd,cjs,mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.0.6",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.1.4",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@cypress/code-coverage": "^3.12.40",
    "@cypress/webpack-batteries-included-preprocessor": "^3.0.3",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@gouvfr/dsfr": "^1.12.1",
    "angular-oauth2-oidc": "^17.0.2",
    "axe-core": "^4.10.3",
    "cypress-axe": "^1.6.0",
    "espression": "^1.8.5",
    "font-awesome": "^4.7.0",
    "font-awesome-animation": "^1.1.1",
    "ngx-logger": "^5.0.12",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.7",
    "@angular/cli": "^18.0.7",
    "@angular/compiler-cli": "^18.0.0",
    "@compodoc/compodoc": "^1.1.25",
    "@cypress/schematic": "^2.5.2",
    "@types/jasmine": "~5.1.0",
    "cypress": "^13.13.0",
    "cypress-fail-fast": "^7.1.0",
    "cypress-html-validate": "^6.1.0",
    "html-validate": "^8.20.1",
    "http-server": "^14.1.1",
    "jasmine-core": "~5.1.0",
    "ng-packagr": "^18.0.0",
    "npm-check-updates": "^16.14.20",
    "source-map-explorer": "^2.5.3",
    "typescript": "~5.4.2"
  }
}
